config rule 'balanced'
	option family 'ipv4'
	option proto 'all'
	option sticky '0'
	option use_policy 'balance'

config rule 'wanfirst'
	option family 'ipv4'
	option proto 'all'
	option sticky '0'
	option ipset 'ct'
	option use_policy 'firstwan'

config rule 'wan_1first'
	option family 'ipv4'
	option proto 'all'
	option sticky '0'
	option ipset 'cnc'
	option use_policy 'firstwan_1'

config rule 'bank'
	option dest_port '80,443'
	option proto 'tcp'
	option sticky '0'
	option use_policy 'firstwan'
	option family 'ipv4'

config policy 'balance'
	list use_member 'wan_m1_w1'
	list use_member 'wan_1_m1_w1'
	option last_resort 'default'

config policy 'firstwan'
	list use_member 'wan_m1_w1'
	list use_member 'wan_1_m2_w1'
	option last_resort 'default'

config policy 'firstwan_1'
	list use_member 'wan_1_m1_w1'
	list use_member 'wan_m2_w1'
	option last_resort 'default'

config globals 'globals'
	option mmx_mask '0x3F00'

config interface 'wan'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '4'
	option interval '10'
	option failure_interval '5'
	option recovery_interval '5'
	option up '5'
	option down '9'
	list track_ip '223.5.5.5'

config interface 'wan_1'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '4'
	option interval '10'
	option failure_interval '5'
	option recovery_interval '5'
	option up '5'
	option down '9'
	list track_ip '223.5.5.5'

config member 'wan_m1_w1'
	option interface 'wan'
	option metric '1'
	option weight '10'

config member 'wan_1_m1_w1'
	option interface 'wan_1'
	option metric '1'
	option weight '5'

config member 'wan_m2_w1'
	option interface 'wan'
	option metric '2'
	option weight '1'

config member 'wan_1_m2_w1'
	option interface 'wan_1'
	option metric '2'
	option weight '1'

